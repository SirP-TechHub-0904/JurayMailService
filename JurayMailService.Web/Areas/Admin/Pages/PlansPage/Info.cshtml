@page
@model JurayMailService.Web.Areas.Admin.Pages.PlansPage.InfoModel
@{
}
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h6 class="card-title">PLAN INFORMATION</h6>


            </div>

            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
                        <div>
                            <div class="row">
                                <div class="col-md-12">
                                    <dl class="row">


                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.Plan.Name)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.Plan.Name)
                                        </dd>

                                         

                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.Plan.Price)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.Raw(Model.Plan.Price)
                                        </dd>



                                    </dl>
                                </div>


                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card-footer">
                <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>
                 
            </div>
        </div>
    </div>
</div>