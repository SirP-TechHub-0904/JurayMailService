    <link href="~/web/style.css" rel="stylesheet">
    <link href="~/web/color.css" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet" />

    <script charset="utf-8" src="~/web/button.856debeac157d9669cf51e73a08fbc93.js"></script>
