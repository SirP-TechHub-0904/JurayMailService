    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Required Meta Tags -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="JBUZZ is Nigeria's leading platform for email and SMS marketing. Send campaigns using your own sender or yourname@jbuzz.africa with ease.">
    <meta name="keywords" content="Email marketing, SMS marketing, bulk SMS, SMTP Nigeria, jbuzz.africa, email blast, campaign tool, JBuzzz, free emails, free SMS">
    <meta name="author" content="JBUZZ Africa">

    <!-- Canonical URL -->
    <link rel="canonical" href="https://jbuzz.africa/" />

    <!-- Site Title -->
    <title>JBUZZ Africa | Smart Email & SMS Marketing Platform</title>

    <!-- Favicons -->
    <link rel="icon" href="~/img/Jbuzz-icon.png" type="image/png">
    <link rel="apple-touch-icon" href="~/img/Jbuzz-icon.png">

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://jbuzz.africa/">
    <meta property="og:title" content="JBUZZ Africa | Smart Email & SMS Marketing">
    <meta property="og:description" content="Start sending emails and SMS for free every month. Use your own sender or @@jbuzz.africa.">
    <meta property="og:image" content="https://jbuzz.africa/assets/social-banner.jpg">

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:url" content="https://jbuzz.africa/">
    <meta name="twitter:title" content="JBUZZ Africa | Smart Email & SMS Marketing">
    <meta name="twitter:description" content="Start sending emails and SMS for free every month. Use your own sender or @@jbuzz.africa.">
    <meta name="twitter:image" content="https://jbuzz.africa/img/logo-white.png">
    <meta name="twitter:creator" content="@@jbuzzafrica">

    <!-- Structured Data (JSON-LD) -->
    <script type="application/ld+json">
        {
          "@@context": "https://schema.org",
          "@@type": "Organization",
          "name": "JBUZZ Africa",
          "url": "https://jbuzz.africa",
          "logo": "https://jbuzz.africa/img/logo-white.png",
          "sameAs": [
            "https://facebook.com/jbuzzafrica",
            "https://twitter.com/jbuzzafrica",
            "https://instagram.com/jbuzz.africa",
            "https://linkedin.com/company/jbuzzafrica"
          ],
          "description": "JBUZZ is Nigeria's go-to email and SMS marketing platform. Send campaigns using your own domain or our secure infrastructure."
        }
    </script>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="~/web/bootstrap.css">

    <!-- FontAwesome -->
    <link rel="stylesheet" href="~/web/font-awesome.min.css">

    <!-- OWL CSS -->
    <link rel="stylesheet" href="~/web/owl.carousel.css">
    <link rel="stylesheet" href="~/web/owl.theme.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css">

    <!-- Custom styles for this template -->
