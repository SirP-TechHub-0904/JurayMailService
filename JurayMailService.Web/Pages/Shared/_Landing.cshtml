    <!-- jQuery Custom
    ================================================== -->
    <script src="~/web/custom.js" type="text/javascript"></script>


    <script>
        $(document).ready(function () {
            $("#jbuzz-testimonials").owlCarousel({
                items: 1,
                loop: true,
                margin: 10,
                autoplay: true,
                autoplayTimeout: 6000,
                autoplayHoverPause: true,
                nav: false,
                dots: true
            });
        });
    </script>



    @await RenderSectionAsync("Scripts", required: false)


